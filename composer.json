{
  "name": "robertfausk/mink-panther-driver",
  "description": "Symfony Panther driver for Mink framework",
  "keywords": [
    "Symfony",
    "Mink",
    "testing",
    "browser"
  ],
  "homepage": "http://mink.behat.org/",
  "type": "mink-driver",
  "license": "MIT",
  "authors": [
    {
      "name": "Robert Freigang",
      "email": "robertfreigang@gmx.de"
    }
  ],
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/robertfausk/panther"
    }
  ],
  "require": {
    "php": ">=7.1",
    "behat/mink": "^1.7.1@dev",
    "symfony/panther": "dev-master",
    "ext-dom": "*"
  },
  "require-dev": {
    "mink/driver-testsuite": "dev-master",
    "symfony/dom-crawler": "~2.3|~3.0|~4.0",
    "symfony/http-kernel": "~2.3|~3.0|~4.0",
    "phpunit/phpunit": "~6.0|~7.0"
  },
  "config": {
    "sort-packages": true
  },
  "suggest": {
    "ext-gd": "*"
  },
  "autoload": {
    "psr-4": {
      "Behat\\Mink\\Driver\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Behat\\Mink\\Tests\\Driver\\": "tests"
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "1.0.x-dev"
    }
  }
}
